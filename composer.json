{
    "name": "turahe/post",
    "description": "A package for managing posts.",
    "type": "library",
    "repositories": [{
        "type": "composer",
        "url": "https://packagist.x-api.app"
    }],
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "turahe/core": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "orchestra/testbench": "^9.6|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Turahe\\Post\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turahe\\Post\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Turahe\\Post\\PostServiceProvider"
            ],
            "aliases": {}
        }
    },
    "authors": [
        {
            "name": "Nur Wachid",
            "email": "wachid@outlook.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit -c phpunit-coverage.xml",
        "pint": "vendor/bin/pint",
        "pint-test": "vendor/bin/pint --test",
        "check": [
            "@pint-test",
            "@test"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
